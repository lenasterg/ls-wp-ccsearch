{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/ls-wp-ccsearch",
  "version": "0.5.0",
  "title": "CC-licensed images",
  "category": "media",
  "icon": "format-image",
  "description": "Easy search and use CC-licensed images for WP helps you search millions of CC-licensed images using the Openverse API and insert the original image into content or set as featured image very quickly.",
  "textdomain": "ls-wp-ccsearch",
  "attributes": {
    "src": {
      "type": "string",
      "selector": "img",
      "attribute": "src",
      "source": "attribute"
    },
	"alt": {
	  "type": "string",
	  "selector": "img",
	  "attribute": "alt",
	  "source": "attribute"
	},
	"prevSearchTerm": {
	  "type": "string"
	}
  },
  "supports": {
    "html": false,
    "lock": true,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "align": true,
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "color": {
      "text": false,
      "background": true,
      "gradients": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
